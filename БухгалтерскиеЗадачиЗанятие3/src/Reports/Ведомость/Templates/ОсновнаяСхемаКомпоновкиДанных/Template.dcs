	ТОГДА Формат(Период,"ДФ=dd.MM.yy") +  " - " + Формат(КонецПериода(Период,"Декада"),"ДФ=dd.MM.yy")
КОГДА &amp;Периодичность = 9  
	ТОГДА Формат(Период,"ДФ='MMMM.yy'") 
КОГДА &amp;Периодичность = 10  
	ТОГДА Формат(Период,"ДФ='k ""кв."" yyyy'")
КОГДА &amp;Периодичность = 11  
	ТОГДА 
		ВЫБОР 
			КОГДА ДеньГода(Период) = 1 
				ТОГДА Формат(Период,"ДФ='""1 полугодие""  yy'")
		ИНАЧЕ Формат(Период,"ДФ='""2 полугодие""  yy'")
		КОНЕЦ
КОГДА &amp;Периодичность = 12 Тогда Формат(Период,"ДФ='yyyy'")
КОНЕЦ
</dcsat:expression>
		</parameter>
	</template>
